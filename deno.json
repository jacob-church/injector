{
    "name": "@cjc/injector",
    "version": "0.3.1",
    "tasks": {
        "dev": "deno run --watch main.ts"
    },
    "fmt": {
        "indentWidth": 4,
        "exclude": [
            ".github/*"
        ]
    },
    "test": {
        "exclude": [
            "./edu/*"
        ]
    },
    "lint": {
        "exclude": ["./edu/*"]
    },
    "imports": {
        "@std/assert": "jsr:@std/assert@1"
    },
    "publish": {
        "include": [
            "LICENSE",
            "README.md",
            "./src/**/*.ts",
            "./index.ts"
        ],
        "exclude": ["./edu", "./src/test/*", "./src/dfs/test.ts"]
    },
    "exports": "./index.ts"
}
